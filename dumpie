window.cursorRadius = 19

function pyth(t,n){return Math.sqrt(t*t+n*n)}
function distance(t,a){return pyth(Math.abs(a.x-t.x),Math.abs(a.y-t.y))}

function clicked() {
    cursor = {
        x: ig.input.mouse.x + ig.game.screen.x,
        y: ig.input.mouse.y + ig.game.screen.y
    };
    
    if (ig.input.state("ctrl")) {
        for (index = 0; index < ig.game.O4114.player.length; ++index) {
            guy = ig.game.O4114.player[index];
            if (distance(guy.pos, cursor) <= cursorRadius) {
                changeBody(guy.attachments.b.id);
                ig.game.sounds.click.play()
            }
        }
    }
}

function changeBody(bodyID) {
    ig.game.O4407.O6333(bodyID);
    //function above is the change body function
}

