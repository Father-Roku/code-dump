!async function clickBodyStealer() {
    /* created by parse w/ love for father ~ <3 */
    if(typeof Deobfuscator === 'undefined')
        await $.getScript('https://cdn.jsdelivr.net/gh/parseml/many-deobf@latest/deobf.js')

    ig.game.sounds.boost.play()

    const cursorRadius = 19
    const pyth = (t, n) => Math.sqrt(t * t + n * n)
    const dist = (t, a) => pyth(Math.abs(a.x - t.x), Math.abs(a.y - t.y))

    document.addEventListener('click', event => {
        const cursor = {
            x: ig.input.mouse.x + ig.game.screen.x,
            y: ig.input.mouse.y + ig.game.screen.y
        }

        if(ig.input.state('ctrl')) {
            updatePlayers()

            ig.game.players.forEach(player => {
                if(dist(player.pos, cursor) <= cursorRadius) {
                    ig.game.sounds.click.play()
                    ig.game.equip.item(player.attachments.b.id)
                }
            })
        }
    })
}()

