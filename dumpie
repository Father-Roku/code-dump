function slowType(fullText, delay){
    i = 0
    totalTextLength = 0
    type = setInterval(function(){
        char = fullText.charAt(i)
        ig.game.O884.O2127("_s"+char)
        i = i+1
        totalTextLength = totalTextLength + 1
        if (totalTextLength > 20 && char == " ") {ig.game.O884.O2127("_nl");totalTextLength=0}
        if (totalTextLength > 20 && char == "~") {ig.game.O884.O2127("_nl");totalTextLength=0}
        if (i>=fullText.length) { clearInterval(type) }
    },delay)
}

slowType(text1, 60)
findVariable: function(afterString) {
        var string = new String();
        var a = this.source.indexOf(afterString);
        var b = new String();
        for(var i = a - 1; ; i--) {
            b  = this.source[i];
            if (b == ".") break;
            string = string + b;
        }
        return string.split("").reverse().join("");
    },
    findFunction: function(insideFunc)  {
        var string = new String();
        var a = this.source.indexOf(insideFunc);
        var b = new String();
        var found = 0;
        for(var j = a; ; j--) {
            if( this.source[j - 0] == "n" &&
                this.source[j - 1] == "u" &&
                this.source[j - 2] == "f" &&
                this.source[j - 3] == ":") {
                found = j - 3;
                break;
            }
        }
        for(var i = found - 1; ; i--) {
            b  = this.source[i];
            if (b == ",") break;
            string = string + b;
        }
        return string.split("").reverse().join("");
    },
